    <script type="text/javascript">
      window.P && P.when('A', 'a-popover', 'ready').execute(function (A, popover) {

        
      (function() {
      function _pldnOnce(fn) {
        var didCall = false;
        return function() {
          if (!didCall) {
            fn.apply(this, arguments);
            didCall = true;
          }
        }
      }

      var trackEndpoints = ['/gp/charity/ajax/track.html/ref=smi_se_rspo_cnt','/gp/charity/ajax/track.html/ref=smi_se_smaf_cnt'];
      A.on("a:popover:show:pldn-xsite-popover", _pldnOnce(function(){
        A.$.each(trackEndpoints, function( index, endpoint ) {
          A.ajax(endpoint);
        });
      }));
  })()


        A.declarative('smile-open-window-sup', 'click', function(event){
          openSocialWindow(event);
        });

        function openSocialWindow(event) {
          var data = event.data;
          if (data.width && data.height) {
            var name = data.name || 'smile-popup';

            name = name.replace(/[\s\-]([a-z])/ig, function( all, letter ) {
                return letter.toUpperCase();
            }).replace(/[\s\-]/g, "");

            window.open(data.url, name, 'width='+data.width+',height='+data.height);
          } else {
            window.open(data.url);
          }
        }
      });
    </script>


  
  
  


